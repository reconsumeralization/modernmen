{
  "name": "chimera-nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:unified": "node scripts/start-unified.js dev",
    "dev:full": "node scripts/start-unified.js full",
    "dev:docs": "node scripts/start-unified.js docs",
    "dev:cms": "node scripts/start-unified.js cms",
    "dev:all": "node scripts/start-unified.js all",
    "dev:status": "node scripts/start-unified.js --status",
    "dev:stop": "node scripts/start-unified.js --stop",
    "build": "next build",
    "build:vercel": "npm run lint:fix && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true next build",
    "db:generate": "supabase gen types typescript --project-id YOUR_PROJECT_ID --schema public > src/types/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "mcp:start": "ts-node src/mcp/bridge-server.ts",
    "docker:build": "docker build -t modernmen .",
    "docker:run": "docker run -p 3000:3000 modernmen",
    "payload": "payload",
    "payload:dev": "payload dev",
    "payload:generate": "payload generate",
    "payload:generate-types": "payload generate:types",
    "payload:init": "curl http://localhost:3000/api/payload/init?action=init",
    "payload:seed": "curl http://localhost:3000/api/payload/init?action=seed",
    "payload:setup": "curl -X POST http://localhost:3000/api/payload/init -H \"Content-Type: application/json\" -d \"{\\\"action\\\":\\\"full-setup\\\"}\"",
    "payload:health": "curl http://localhost:3000/api/payload/init?action=health",
    "tools:generate-types": "ts-node src/tools/generate-payload-types.ts",
    "tools:seed-db": "ts-node src/tools/seed-database.ts",
    "tools:lint-fix": "ts-node src/tools/run-lint-fix.ts",
    "tools:export-docs": "ts-node src/tools/export-docs.ts",
    "tools:deploy": "ts-node src/tools/deploy-vercel.ts"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.10.0",
