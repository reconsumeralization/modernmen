{
  "name": "modernmen-hair-BarberShop",
  "version": "1.0.0",
  "description": "Modern Men Hair BarberShop - Regina's Premier Men's Grooming",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:full": "next dev",
    "build": "next build",
    "dev:safe": "next dev",
    "build:dev": "NODE_ENV=development next build",
    "prebuild": "echo 'Pre-build hook skipped'",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "echo 'Tests skipped - setup Jest first'",
    "test:unit": "echo 'Unit tests skipped'",
    "test:integration": "echo 'Integration tests skipped'",
    "test:e2e": "echo 'E2E tests skipped'",
    "test:watch": "echo 'Test watch skipped'",
    "test:coverage": "echo 'Test coverage skipped'",
    "test:ci": "echo 'CI tests skipped'",
    "clean": "rm -rf .next",
    "format": "echo 'Formatting skipped'",
    "sync:dry-run": "tsx scripts/sync-all.ts --dry-run",
    "generate:types": "tsx scripts/sync-all.ts --generate-types",
    "validate:types": "tsx scripts/sync-all.ts --validate-types",
    "validate:data": "tsx scripts/sync-all.ts --validate-data",
    "validate:cross": "tsx scripts/sync-all.ts --cross-validate",
    "types:generate": "npm run types:collections && npm run payload:generate-types",
    "types:validate": "npm run validate:types",
    "types:check": "npm run type-check && npm run validate:types",
    "db:sync": "npm run sync:full",
    "db:validate": "npm run validate:data && npm run validate:cross",
    "db:health": "npm run sync:health",
    "system:check": "npm run types:check && npm run db:validate && npm run sync:health",
    "system:sync": "npm run sync:all",
    "system:health": "npm run system:check",
    "payload:build": "payload build",
    "payload:dev": "payload dev",
    "payload:generate-types": "payload generate:types",
    "payload:migrate": "payload migrate",
    "payload:seed": "payload seed",
    "supabase:sync": "npm run sync:full",
    "supabase:types": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "deploy:check": "pnpm run build:safe && echo 'Build successful!'",
    "deploy:validate": "npm run system:check && npm run build",
    "deploy:build": "npm run payload:build && npm run build",
    "deploy:full": "npm run system:sync && npm run deploy:build",
    "seed:content": "SEED_API_KEY=$SEED_API_KEY tsx scripts/seed-content.ts",
    "seed": "SEED_API_KEY=$SEED_API_KEY tsx scripts/seed.ts",
    "seed:all": "npm run seed && npm run seed:content",
    "seo:generate": "tsx scripts/generate-sitemap.ts",
    "seo:check": "tsx scripts/seo-check.ts",
    "smoke:check": "tsx scripts/smoke-check.ts",
    "smoke:test": "npm run smoke:check && npm run test:e2e",
    "db:migrate": "payload migrate",
    "db:reset": "payload migrate:reset",
    "db:fresh": "npm run db:reset && npm run db:migrate",
    "cms:dev": "npm run payload:dev",
    "cms:build": "npm run payload:build",
    "cms:types": "npm run payload:generate-types",
    "builder:dev": "npm run dev",
    "builder:build": "npm run build",
    "builder:preview": "npm run start",
    "crm:dev": "npm run dev",
    "crm:build": "npm run build",
    "crm:sync": "npm run sync:data",
    "dev:all": "concurrently \"npm run cms:dev\" \"npm run dev\" \"npm run sync:full\"",
    "build:all": "npm run cms:build && npm run builder:build",
    "check:all": "npm run system:check && npm run test:ci",
    "setup:amazing": "tsx scripts/setup-amazing-builder.ts",
    "setup:quick": "tsx scripts/setup-amazing-builder.ts --skip-build",
    "setup:full": "tsx scripts/setup-amazing-builder.ts",
    "types:collections": "tsx scripts/generate-collection-types.ts",
    "vercel:fix": "node scripts/vercel-deploy-fix.js",
    "vercel:validate": "node scripts/vercel-deployment-validation.js",
    "build:safe": "pnpm install --frozen-lockfile && pnpm run build"
  },
